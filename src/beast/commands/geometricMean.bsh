// @see(doc/html/beast/base/StatUtils.html)

geometricMean(x) {
	double [] y = toDoubleArray(x);
	return org.apache.commons.math3.stat.StatUtils.geometricMean(y);
}

geometricMean(x, int begin, int end) {
	double [] y = toDoubleArray(x);
	return org.apache.commons.math3.stat.StatUtils.geometricMean(y, begin, length);
}
