// @see(doc/html/beast/stats/Logistic.html) The Logistic Distribution

plogis(x, location, scale) {
	if (x instanceof java.util.List || x.getClass().isArray()) {
		java.util.List<Double> result = new ArrayList<Double>();
		for (i : x) {
			result.add(org.renjin.Distributions.plogis(i, location, scale, false, false));
		}
		return result;
	} else {
		return org.renjin.Distributions.plogis(x, location, scale, false, false);
	}
}

