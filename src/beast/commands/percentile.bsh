// @see(doc/html/beast/base/StatUtils.html)

percentile(x, double p) {
	double [] y = toDoubleArray(x);
	return org.apache.commons.math3.stat.StatUtils.percentile(y, p);
}

percentile(x, int begin, int length, double p) {
	double [] y = toDoubleArray(x);
	return org.apache.commons.math3.stat.StatUtils.percentile(y, begin, length, p);
}


