sumSq(sample) {
	double [] x = toDoubleArray(sample);
	return org.apache.commons.math3.stat.StatUtils.sumSq(x);
}

sumSq(sample, int begin, int end) {
	double [] x = toDoubleArray(sample);
	return org.apache.commons.math3.stat.StatUtils.sumSq(x, begin, length);
}
