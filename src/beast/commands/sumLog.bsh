// @see(doc/html/beast/base/StatUtils.html)

sumLog(sample) {
	double [] x = toDoubleArray(sample);
	return org.apache.commons.math3.stat.StatUtils.sumLog(x);
}

sumLog(sample, int begin, int end) {
	double [] x = toDoubleArray(sample);
	return org.apache.commons.math3.stat.StatUtils.sumLog(x, begin, length);
}
