mode(sample) {
	double [] x = toDoubleArray(sample);
	return org.apache.commons.math3.stat.StatUtils.mode(x);
}

mode(sample, int begin, int end) {
	double [] x = toDoubleArray(sample);
	return org.apache.commons.math3.stat.StatUtils.mode(x, begin, length);
}
